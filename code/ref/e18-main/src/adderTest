    %delay 1000, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55a01233eb50;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55a012356050_0, 0, 1;
    %pushi/vec4 1092616192, 0, 32;
